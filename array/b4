#include <stdio.h>
#include <stdlib.h>
int main()
{
    int n;
    printf("Chuoi dai bao nhieu so "),scanf("%d",&n);
    int *arr =(int*)malloc(n*sizeof(int));
    for(int i=0;i<n;i++)
    printf("Nhap so thu %d ",i+1),scanf("%d",&arr[i]);
    int min=arr[0];int max=arr[0];
    for(int i=1;i<n;i++)
    {
        if(arr[i]<min)
        min=arr[i];
        if(arr[i]>max)
        max=arr[i];
    }
    free(arr);
    printf("Max la %d ",max);
    printf("Min la %d ",min);
}
