#include <stdio.h>
int main()
{
    int x,y,a,b,gcd;
    printf("Nhap so dau tien "),scanf("%d",&x);
    printf("Nhap so thu hai "),scanf("%d",&y);
    a = x;
    b = y;
    if(x<y)
    {
        int temp = y;
        y = x;
        x = temp;
    }
    while (y!=0)
    {
        gcd = y;
        y=x%y;
        x = gcd;
    }
    printf("UCLN la %d\n",gcd);
    printf("BCNN la %d",(a/gcd)*b);
}
